  "spec/features/*_spec.rb": {
    "type": "feature",
    "template": ["require \"rails_helper\"","","feature \"{blank}\" do", "end"]
  },
  "spec/pages/*.rb": {
    "type": "page",
    "template": [ "module Pages", "  class {camelcase}", "    include Capybara::DSL", "", "  end", "end"]
  },
  "webpack/assets/javascripts/components/*.jsx": {
